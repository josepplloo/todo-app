{
  "name": "todo-app",
  "version": "1.0.0",
  "description": "JS to do list",
  "private": true,
  "scripts": {
    "build": "webpack",
    "start": "webpack-dev-server --open --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/josepplloo/todo-app.git"
  },
  "author": "josepplloo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/josepplloo/todo-app/issues"
  },
  "homepage": "https://github.com/josepplloo/todo-app#readme",
  "dependencies": {
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1"
  }
}
